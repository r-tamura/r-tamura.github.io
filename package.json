  "ava": {
    "files": [
      "**/__test__/**/*.js"
    ],
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ],
      "compileAsTests": [
        "src/**/*.js"
      ],
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
