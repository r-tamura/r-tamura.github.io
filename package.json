    "create-post": "node utils/create-post",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "ava",
    "deploy": "node ./utils/deploy.js",
    "clean": "node ./utils/clean.js",
    "serve": "gatsby serve"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "ava": "^3.15.0",
    "gh-pages": "^3.2.3",
    "luxon": "^2.0.2",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
